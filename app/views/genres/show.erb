<!DOCTYPE html>
<html>
<head>
	<title><%= @genre.name %></title>
</head>
<body>
	<h1><%= @genre.name %></h1>
	<h2>Artists</h2>
	<% @genre.artists.each do |a| %>
	<p><a href = "/artists/<%=a.slug%>"><%= a.name %></a></p>
	<% end %>
	<h2>Songs</h2>
	<% @genre.songs.each do |s| %>
	<p><a href = "/songs/<%=s.slug%>"><%= s.name %></a></p>
	<% end %>
</body>
</html>